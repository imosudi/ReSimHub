{
  "id": null,
  "uid": "resimhub-observe",
  "title": "üìä ReSimHub Observability Dashboard",
  "tags": ["resimhub", "reinforcement-learning", "metrics"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "style": "dark",
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "üß† Active RL Experiments",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "rl_experiments_active",
          "legendFormat": "Active Experiments",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 8, "h": 6 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "text": {}
      }
    },
    {
      "id": 2,
      "type": "graph",
      "title": "üèÅ Completed Training Runs (per minute)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "increase(rl_training_completed_total[1m])",
          "legendFormat": "Completed Trainings",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 8, "y": 0, "w": 16, "h": 6 },
      "lines": true,
      "fill": 2,
      "linewidth": 2,
      "points": false,
      "yaxes": [
        {
          "format": "short",
          "label": "Training Runs",
          "logBase": 1
        },
        {
          "format": "short"
        }
      ],
      "legend": {
        "show": true,
        "values": false
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "‚è±Ô∏è Average Benchmark Latency (ms)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "rl_avg_benchmark_latency_ms",
          "legendFormat": "Avg Latency (ms)",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 7, "w": 24, "h": 7 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 40 },
              { "color": "red", "value": 60 }
            ]
          },
          "unit": "ms"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "job",
        "type": "query",
        "label": "Job",
        "datasource": {
          "type": "prometheus",
          "uid": "Prometheus"
        },
        "query": "label_values(job)",
        "current": {
          "selected": true,
          "text": "resimhub",
          "value": "resimhub"
        }
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "type": "dashboard",
        "name": "Annotations & Alerts",
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)"
      }
    ]
  },
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
  }
}
